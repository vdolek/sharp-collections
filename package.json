{
  "name": "sharp-collections",
  "version": "1.7.0",
  "description": ".NET Linq like collection library for TypeScript and JavaScript.",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "prepublish": "npm run build",
    "test": "ts-mocha --paths -p tsconfig.tests.json 'tests/**/*.spec.ts'",
    "postbuild": "sed -i '7 a // @ts-ignore' dist/collections/ReadOnlyDictionary.d.ts"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "Linq",
    "Typescript",
    "Collections",
    "List",
    "LinkedList",
    "Dictionary",
    "HashSet",
    "Stack",
    "Queue",
    "ReadOnly",
    "HashCode",
    "EqualityComparer"
  ],
  "author": "Martin Volek",
  "license": "MIT",
  "homepage": "https://github.com/vdolek/sharp-collections#readme",
  "dependencies": {
    "@types/fast-json-stable-stringify": "^2.0.0",
    "deep-equal": "^2.0.5",
    "fast-json-stable-stringify": "^2.1.0"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "chai": "^4.3.0",
    "cross-env": "^7.0.3",
    "mocha": "^8.3.0",
    "mocha-param": "^2.0.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~6.1.0",
    "tslint-eslint-rules": "~5.4.0",
    "tslint-microsoft-contrib": "~6.2.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "tslint-teamcity-reporter": "~3.2.2",
    "typescript": "^4.2.2"
  }
}
